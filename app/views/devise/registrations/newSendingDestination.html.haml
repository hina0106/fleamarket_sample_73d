%h2 発送元・お届け先住所
= form_for @sendingDestination do |f|
  = render "devise/shared/error_messages", resource: @sendingDestination
  .field
    = f.label :destination_first_name
    %br/
    = f.text_field :destination_first_name, value: 'あ'
  .field
    = f.label :destination_family_name
    %br/
    = f.text_field :destination_family_name, value: 'あ'
  .field
    = f.label :destination_first_name_kana
    %br/
    = f.text_field :destination_first_name_kana, value: 'あ'
  .field
    = f.label :destination_family_name_kana
    %br/
    = f.text_field :destination_family_name_kana, value: 'あ'
  .field
    = f.label :post_code
    %br/
    = f.text_field :post_code, id: 'postcode', value: '6330003'
  .field
    = f.label :prefecture_code
    %br/
    = f.collection_select :prefecture_code, JpPrefecture::Prefecture.all, :code, :name, id: 'address_prefecture_code', include_blank: '都道府県'
  .field
    = f.label :city
    %br/
    = f.text_field :city, id:'address_city'
  .field
    = f.label :house_number
    %br/
    = f.text_field :house_number, id: 'address_street', value: '1'
  .field
    = f.label :building_name
    %br/
    = f.text_field :building_name
  .field
    = f.label :phone_number
    %br/
    = f.text_field :phone_number
  .actions
    = f.submit "Sign up"
= render "devise/shared/links"
